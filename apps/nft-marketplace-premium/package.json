{
  "name": "dexappbuilder",
  "version": "0.21.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/update-version-json.js",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "test": "jest",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "process-all-translations": "./scripts/bash/process-all-translations.sh",
    "translate-ai": "node --env-file=.env scripts/translate.js",
    "sync-language-files": "node scripts/sync-compiled-with-lang.js",
    "lang-diff": "node scripts/local-diff.js",
    "generate-lang-files": "yarn extract '(src||pages)/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/main.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
  },
  "pre-push": [
    "build"
  ],
  "browser": {
    "child_process": false,
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "dgram": false,
    "module": false
  },
